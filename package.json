{
  "name": "fluidamente-dashboard",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test:webhook:valid": "bash ./scripts/test-webhook.sh valid",
    "test:webhook:invalid": "bash ./scripts/test-webhook.sh invalid",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@nextui-org/react": "^2.2.9",
    "@types/nodemailer": "^6.4.15",
    "framer-motion": "^10.16.16",
    "handlebars": "^4.7.8",
    "husky": "^9.0.7",
    "iconsax-react": "^0.0.8",
    "mercadopago": "^2.10.1",
    "next": "14.0.4",
    "nodemailer": "^6.9.14",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.1",
    "sonner": "^1.5.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.7.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "commitlint": "^17.7.1",
    "encoding": "^0.1.13",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-tsc": "^2.0.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.32",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "yarn format"
    ],
    "./**/*.json": "yarn format",
    "./**/*.{ts,tsx}": [
      "eslint --fix --plugin tsc --rule 'tsc/config: [2, {configFile: ./tsconfig.json}]'"
    ]
  }
}
